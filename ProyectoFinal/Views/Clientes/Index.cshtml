@model List<ProyectoFinal.Models.Clientes>
@{
    ViewBag.Title = "Cliente Particular";
}

<h2>Lista de Clientes particulares</h2>
<div class="row">
    <h3>@Html.ActionLink("Crear", "New")</h3>
</div>

<div>
    @foreach (var data in Model)
    {
        <ul>
            <li>@data.Nombre</li>
            <li>@data.Provincia</li>
            <li>@data.Telefono</li>
        
        
        </ul>

}

</div>


